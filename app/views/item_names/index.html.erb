<p id="notice"><%= notice %></p>

<h1>Listing Item Names</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @item_names.each do |item_name| %>
      <tr>
        <td><%= item_name.name %></td>
        <td><%= link_to 'Show', item_name %></td>
        <td><%= link_to 'Edit', edit_item_name_path(item_name) %></td>
        <td><%= link_to 'Destroy', item_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item name', new_item_name_path %>
